<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record model="ir.model" id="model_presale_order_line">
        <field name="name">Presale order line</field>
        <field name="model">x_presale.order.line</field>
    </record>

    <record model="ir.model.fields" id="field_presale_order_line_product_id">
        <field name="name">x_product_id</field>
        <field name="field_description">Product id</field>
        <field name="model_id" ref="model_presale_order_line" />
        <field name="ttype">many2one</field>
        <field name="relation">product.product</field>
    </record>

    <record model="ir.model.fields" id="field_presale_order_line_quantity">
        <field name="name">x_quantity</field>
        <field name="field_description">Quantity</field>
        <field name="model_id" ref="model_presale_order_line" />
        <field name="ttype">float</field>
    </record>

    <record model="ir.model.fields" id="field_presale_order_line_price">
        <field name="name">x_price</field>
        <field name="field_description">Price</field>
        <field name="model_id" ref="model_presale_order_line" />
        <field name="ttype">float</field>
    </record>

</odoo>